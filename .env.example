# MongoDB Connection String (sesuai GitHub Secret: MONGOSTRING)
MONGOSTRING=mongodb+srv://username:password@cluster.mongodb.net/database?retryWrites=true&w=majority

# JWT Secret for Authentication (sesuai GitHub Secret: PRKEY - hex format)
PRKEY=your-hex-prkey-from-github-secret

# Fallback (optional)
MONGODB_URI=mongodb+srv://username:password@cluster.mongodb.net/database?retryWrites=true&w=majority
JWT_SECRET=your-super-secret-jwt-key-change-this-in-production

# Node Environment
NODE_ENV=development
